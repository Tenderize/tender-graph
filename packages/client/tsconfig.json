{
    "compilerOptions": {
        //"target": "es2016",
        "module": "CommonJS",
        "moduleResolution": "nodenext",
        "forceConsistentCasingInFileNames": true,
        "esModuleInterop": true,
        "strict": true,
        "skipLibCheck": true,
        "resolveJsonModule": true,
        "downlevelIteration": true,
        "allowSyntheticDefaultImports": true,
        "declaration": true,
        "declarationMap": true,
        "sourceMap": true,
    }
}