{
    "exclude": [
        "node_modules"
    ],
    "compilerOptions": {
        "target": "es2016",
        "module": "commonjs",
        "forceConsistentCasingInFileNames": true,
        "esModuleInterop": true,
        "strict": true,
        "skipLibCheck": true,
        "resolveJsonModule": true,
        "downlevelIteration": true,
        "allowSyntheticDefaultImports": true,
        "declaration": true,
        "declarationMap": true,
        "sourceMap": true,
    }
}